{
  "name": "my-nextjs-app",
  "version": "0.1.0",
  "private": true,
  "main": "main.js",
  "scripts": {
    
    "dev": "powershell -Command \"Start-Process http://localhost:3000; next dev\"",
    "build": "next build",
    "start": "powershell -Command \"Start-Process http://localhost:3000;\" & next start  ",
    "lint": "next lint",
