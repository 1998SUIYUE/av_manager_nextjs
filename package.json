    "clean": "rimraf .next"
  },
  "dependencies": {
    "axios": "^1.7.8",
    "cheerio": "^1.0.0",
    "next": "15.0.3",
    "playwright": "^1.49.0",
    "react": "19.0.0-rc-66855b96-20241106",
    "react-dom": "19.0.0-rc-66855b96-20241106"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "electron": "^33.2.1",
    "electron-builder": "^25.1.8",
    "eslint": "^8",
    "eslint-config-next": "15.0.3",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  },
  "build": {
    "appId": "AVManger",
    "files": [
      "main.js",
      "out/**/*"
    ],
    "directories": {
      "buildResources": "assets"
